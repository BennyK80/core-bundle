{% extends 'WebProfilerBundle:Profiler:layout.html.twig' %}

{% block toolbar %}
    {% set icon %}
        <span>
        <img width="32" height="28" alt="Contao" src="data:image/png;base64,">
        {% if collector.summary.scope %}
            <span class="sf-toolbar-info-piece-additional sf-toolbar-status">{{ collector.summary.scope }}</span>
        {% endif %}
        </span>
    {% endset %}
    {% set text %}
    <div class="sf-toolbar-info-piece">
        <b>Framework</b>
        {% if collector.summary.framework %}Contao &amp; {% endif %}Symfony
    </div>
    {% if collector.summary.scope %}
    <div class="sf-toolbar-info-piece">
        <b>Container Scope</b>
        {{ collector.summary.scope }}
    </div>
    {% endif %}
    <div class="sf-toolbar-info-piece">
        <b>Contao Modules</b>
        {% if collector.summary.modules %}
            <span class="sf-toolbar-status sf-toolbar-status-red">
                <abbr title="{{ collector.summary.modules|join(', ') }}">{{ collector.summary.modules|length }}</abbr>
            </span>
        {% else %}
            <span class="sf-toolbar-status sf-toolbar-status-green">0</span>
        {% endif %}
    </div>
    {% endset %}
    {% if collector.summary.framework %}
        {% include '@WebProfiler/Profiler/toolbar_item.html.twig' with { 'link': profiler_url } %}
    {% else %}
        {% include '@WebProfiler/Profiler/toolbar_item.html.twig' with { 'link': false } %}
    {% endif %}
{% endblock %}

{% block menu %}
{% if collector.summary.framework %}
    <span class="label">
        <span class="icon"><img src="data:image/png;base64," alt=""></span>
        <strong>Contao</strong>
    </span>
{% endif %}
{% endblock %}

{% block panel %}
{% if collector.summary.framework %}
    <h2>Summary</h2>
    <table>
        <tr>
            <th>Container Scope</th>
            <td>
                {% if collector.summary.scope %}
                    <span class="sf-toolbar-status sf-toolbar-status-green">{{ collector.summary.scope }}</span>
                {% else %}
                    <span class="sf-toolbar-status sf-toolbar-status-red">unknown</span>
                {% endif %}
            </td>
        </tr>
        <tr>
            <th>Registered models</th>
            <td>{{ collector.summary.models }}</td>
        </tr>
    </table>

    <h2>Unknown Insert Tags</h2>
    {% if collector.unknowninserttags %}
        <table>
            <tr>
                <th>Insert Tag</th>
            </tr>
            {% for name in collector.unknowninserttags %}
                <tr>
                    <td><pre>{{ '{{' }}{{ name }}{{ '}}' }}</pre></td>
                </tr>
            {% endfor %}
        </table>
    {% else %}
        <p><em>No unknown insert tags used on the page.</em></p>
    {% endif %}

    <h2>Unknown Insert Tag Flags</h2>
    {% if collector.unknowninserttagflags %}
        <table>
            <tr>
                <th>Insert Tag Flag</th>
            </tr>
            {% for name in collector.unknowninserttagflags %}
                <tr>
                    <td><pre>{{ name }}</pre></td>
                </tr>
            {% endfor %}
        </table>
    {% else %}
        <p><em>No unknown insert tag flags used on the page.</em></p>
    {% endif %}

    <h2>Classes Aliased</h2>
    <table>
        <tr>
            <th>Alias name</th>
            <th>Original name</th>
        </tr>
        {% for class in collector.classesaliased %}
            <tr>
                <td><pre>{{ class.alias }}</pre></td>
                <td><pre>{{ class.original }}</pre></td>
            </tr>
        {% endfor %}
    </table>

    <h2>Classes Set</h2>
    <table>
        <tr>
            <th>Class name</th>
        </tr>
        {% for class in collector.classesset %}
        <tr>
            <td><pre>{{ class }}</pre></td>
        </tr>
        {% endfor %}
    </table>

    {% if collector.additionaldata %}
    <h2>Other</h2>
    <pre>
    {{ dump(collector.additionaldata) }}
    </pre>
    {% endif %}
{% endif %}
{% endblock %}
